#include <iostream>
#include <string>
#include <stack>
#include <algorithm>
#include <cmath>

using namespace std;
int A[1001][4];
int D[1001][4];
int function(int n){

    for (int i = 1; i <= n; i++){
    
        D[i][1] = D[i-1][2] + D[i-1][3]; 

    }
    

}

int main(){

int n;
cin >> n;

ios_base:: sync_with_stdio(false);
cin.tie(NULL);
cout.tie(NULL);

for (int i = 1; i <= n; i++){
    for(int j = 1; j <=3; j++){
        cin >> D[n][j];    
    }

}

cout << function(n);

return 0;
}